[
      {
        "sonaId": "1",
        "text": "النوم على وضوء"
      },
      {
        "sonaId": "2",
        "text": "قراءة سورة الإخلاص ، والمعوذتين قبل النوم"
      },
      {
        "sonaId": "3",
        "text": "التكبير والتسبيح عند المنام"
      },
      {
        "sonaId": "4",
        "text": "الدعاء حين الاستيقاظ أثناء النوم"
      },
      {
        "sonaId": "5",
        "text": "الدعاء عند الاستيقاظ من النوم بالدعاء الوارد"
      },
      {
        "sonaId": "6",
        "text": "المضمضة والاستنشاق من غرفة واحد"
      },
      {
        "sonaId": "7",
        "text": "الوضوء قبل الغسل"
      },
      {
        "sonaId": "8",
        "text": "التشهد بعد الوضوء"
      },
      {
        "sonaId": "9",
        "text": "الاقتصاد في الماء"
      },
      {
        "sonaId": "10",
        "text": "صلاة ركعتين بعد الوضوء"
      },
      {
        "sonaId": "11",
        "text": "الترديد مع المؤذن ثم الصلاة على النبي صلى الله عليه وسلم"
      },
      {
        "sonaId": "12",
        "text": "الإكثار من السواك"
      },
      {
        "sonaId": "13",
        "text": "التبكير إلى المسجد"
      },
      {
        "sonaId": "14",
        "text": "الذهاب إلى المسجد ماشيا"
      },
      {
        "sonaId": "15",
        "text": "إتيان الصلاة بسكينة ووقار"
      },
      {
        "sonaId": "16",
        "text": "الدعاء عند دخول المسجد ، و الخروج منه"
      },
      {
        "sonaId": "17",
        "text": "الصلاة إلى سترة"
      },
      {
        "sonaId": "18",
        "text": "الإقعاء بين السجدتين"
      },
      {
        "sonaId": "19",
        "text": "التورك في التشهد الثاني"
      },
      {
        "sonaId": "20",
        "text": "الإكثار من الدعاء قبل التسليم"
      },
      {
        "sonaId": "21",
        "text": "أداء السنن الرواتب"
      },
      {
        "sonaId": "22",
        "text": "صلاة الضحى"
      },
      {
        "sonaId": "23",
        "text": "قيام الليل"
      },
      {
        "sonaId": "24",
        "text": "صلاة الوتر"
      },
      {
        "sonaId": "25",
        "text": "الصلاة في النعلين إذا تحققت طهارتهما"
      },
      {
        "sonaId": "26",
        "text": "الصـلاة في مسجد قباء"
      },
      {
        "sonaId": "27",
        "text": "أداء صلاة النافلة في البيت"
      },
      {
        "sonaId": "28",
        "text": "صلاة الاستخارة"
      },
      {
        "sonaId": "29",
        "text": "الجلوس في المصلى بعد صلاة الفجر حتى تطلع الشمس"
      },
      {
        "sonaId": "30",
        "text": "الاغتسال يوم الجمعة"
      },
      {
        "sonaId": "31",
        "text": "التبكير إلى صلاة الجمعة"
      },
      {
        "sonaId": "32",
        "text": "تحري ساعة الإجابة يوم الجمعة"
      },
      {
        "sonaId": "33",
        "text": "الذهاب إلى مصلى العيد من طريق، والعودة من طريق آخر"
      },
      {
        "sonaId": "34",
        "text": "الصلاة على الجنازة"
      },
      {
        "sonaId": "35",
      "text": "زيارة المقابر"
      },
      {
        "sonaId": "36",
        "text": "السحور"
      },
      {
        "sonaId": "37",
        "text": "تعجيل الفطر ، وذلك إذا تحقق غروب الشمس"
      },
      {
        "sonaId": "38",
        "text": "قيام رمضان"
      },
      {
        "sonaId": "39",
        "text": "الاعتكاف في رمضان ، وخاصة في العشر الأواخر منه"
      },
      {
        "sonaId": "40",
        "text": "صوم ستة أيام من شوال"
      },
      {
        "sonaId": "41",
        "text": "صوم ثلاثة أيام من كل شهر"
      },
      {
        "sonaId": "42",
        "text": "صوم يوم عرفة"
      },
      {
        "sonaId": "43",
        "text": "صوم يوم عاشوراء"
      },
      {
        "sonaId": "44",
        "text": "اختيار أمير في السفر"
      },
      {
        "sonaId": "45",
        "text": "التكبير عند الصعود والتسبيح عند النزول"
      },
      {
        "sonaId": "46",
        "text": "الدعاء حين نزول منزل"
      },
      {
        "sonaId": "47",
        "text": "البدء بالمسجد إذا قدم من السفر"
      },
      {
        "sonaId": "48",
        "text": "الدعاء عند لبس ثوب جديد"
      },
      {
        "sonaId": "49",
        "text": "لبس النعل باليمين"
      },
      {
        "sonaId": "50",
        "text": "التسمية عند الأكل"
      },
      {
        "sonaId": "51",
        "text": "حمد الله بعد الأكل والشرب"
      },
      {
        "sonaId": "52",
        "text": "الجلوس عند الشرب"
      },
      {
        "sonaId": "53",
        "text": "المضمضة من اللبن"
      },
      {
        "sonaId": "54",
        "text": "عدم عيب الطعام"
      },
      {
        "sonaId": "55",
        "text": "الأكل بثلاثة أصابع"
      },
      {
        "sonaId": "56",
        "text": "الشرب والاستشفاء من ماء زمزم"
      },
      {
        "sonaId": "57",
        "text": "الأكل يوم عيد الفطر قبل الذهاب للمصلى"
      },
      {
        "sonaId": "58",
        "text": "الإكثار من قراءة القرآن"
      },
      {
        "sonaId": "59",
        "text": "تحسين الصوت بقراءة القرآن"
      },
      {
        "sonaId": "60",
        "text": "ذكر الله على كل حال"
      },
      {
        "sonaId": "61",
        "text": "التسبيح"
      },
      {
        "sonaId": "62",
        "text": "تشميت العاطس"
      },
      {
        "sonaId": "63",
        "text": "الدعاء للمريض"
      },
      {
        "sonaId": "64",
        "text": "وضع اليد على موضع الألم ، مع الدعاء"
      },
      {
        "sonaId": "65",
        "text": "الدعاء عند سماع صياح الديك ، والتعوذ عند سماع نهيق الحمار"
      },
      {
        "sonaId": "66",
        "text": "الدعـاء عند نزول المطر"
      },
      {
        "sonaId": "67",
        "text": "ذكر الله عند دخول المنزل"
      },
      {
        "sonaId": "68",
        "text": "ذكر الله في المجلس"
      },
      {
        "sonaId": "69",
        "text": "الدعاء عند دخول الخلاء"
      },
      {
        "sonaId": "70",
        "text": "الدعاء عندما تعصف الريح"
      },
      {
        "sonaId": "71",
        "text": "الدعاء عندما تعصف الريح"
      },
      {
        "sonaId": "72",
        "text": "الدعـاء عند المصيبة"
      },
      {
        "sonaId": "73",
        "text": "إفشاء السلام"
      },
      {
        "sonaId": "74",
        "text": "طلب العلم"
      },
      {
        "sonaId": "75",
        "text": "الاستئذان قبل الدخول ثلاثاً"
      },
      {
        "sonaId": "76",
        "text": "تحنيك المولود"
      },
      {
        "sonaId": "77",
        "text": "العقيقة عن المولود"
      },
      {
        "sonaId": "78",
        "text": "كشف بعض البدن ليصيبه المطر"
      },
      {
        "sonaId": "79",
        "text": "عيادة المريض"
      },
      {
        "sonaId": "80",
        "text": "التبسم"
      },
      {
        "sonaId": "81",
        "text": "التزاور في الله"
      },
      {
        "sonaId": "82",
        "text": "إعلام الرجل أخيه أنه يحبه"
      },
      {
        "sonaId": "83",
        "text": "رد التثاؤب"
      },
      {
        "sonaId": "84",
        "text": "إحسان الظن بالناس"
      },
      {
        "sonaId": "85",
        "text": "معاونة الأهل في أعمال المنزل"
      },
      {
        "sonaId": "86",
        "text": "سُنن الفطرة"
      },
      {
        "sonaId": "87",
        "text": "كفالة اليتيم"
      },
      {
        "sonaId": "88",
        "text": "تجنب الغضب"
      },
      {
        "sonaId": "89",
        "text": "البكاء من خشية الله"
      },
      {
        "sonaId": "90",
        "text": "الصدقة الجارية"
      },
      {
        "sonaId": "91",
        "text": "بناء المساجد"
      },
      {
        "sonaId": "92",
        "text": "السماحة في البيع والشراء"
      },
      {
        "sonaId": "93",
        "text": "إزالة الأذى عن الطريق"
      },
      {
        "sonaId": "94",
        "text": "الصدقة"
      },
      {
        "sonaId": "95",
        "text": "الإكثار من الأعمال الصالحة في عشر ذي الحجة"
      },
      {
        "sonaId": "96",
        "text": "قتل الوزغ"
      },
      {
        "sonaId": "97",
        "text": "النهي عن أن يُحَدِّث المرء بكل ما سمع"
      },
      {
        "sonaId": "98",
        "text": "احتساب النفقة على الأهل"
      },
      {
        "sonaId": "99",
        "text": "الرَّمَل في الطواف"
      },
      {
        "sonaId": "100",
        "text": "المداومة على العمل الصالح وإن قل"
      }
]
