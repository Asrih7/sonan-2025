[
  {
    "id": 1,
    "text": "سمِع الله لمن حمِده",
    "source": "رواه البخاري، ومسلم"
  },
  {
    "id": 2,
    "text": "فإذا استوى قائمًا، قال: ربَّنا لك الحمد",
    "source": "رواه البخاري، ومسلم"
  },
  {
    "id": 3,
    "text": "ربنا ولك الحمد حمدًا كثيرًا طيبًا مباركًا فيه",
    "source": "رواه البخاري"
  },
  {
    "id": 4,
    "text": "ملء السماوات وملء الأرض، وما بينهما وملء ما شِئت من شيء بعد",
    "source": "رواه مسلم"
  },
  {
    "id": 5,
    "text": "لربي الحمد لربي الحمد، يُكرِّرها",
    "source": "رواه أبو داود"
  }
]