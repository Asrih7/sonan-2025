 [
      {
        "adkharId":"1",
        "text": "أذكار الصباح والمساء"
      },
      {
        "adkharId":"2",
        "text": "ما يقول إذا استيقَظ من منامه"
      },
      {
        "adkharId":"3",
        "text": "ما يقول إذا لبِس ثوبه"
      },
      {
        "adkharId":"4",
        "text": "كيفية لُبس الثوب"
      },
      {
        "adkharId":"5",
        "text": "ما يقول إذا دخل الخلاء"
      },
      {
        "adkharId":"6",
        "text": "ما يقول إذا خرج من الخلاء"
      },
      {
        "adkharId":"7",
        "text": "الذكر قبل الوضوء"
      },
      {
        "adkharId":"8",
        "text": "ما يقول إذا فرغ من وضوئه"
      },
      {
        "adkharId":"9",
        "text": "ما يقال عند الخروج من المنزل"
      },
      {
        "adkharId":"10",
        "text": "ما يقال عند الدخول إلى المنزل"
      },
      {
        "adkharId":"11",
        "text": "ما يقول عند دخول المسجد"
      },
      {
        "adkharId":"12",
        "text": "ما يقول عند الخروج من المسجد"
      },
      {
        "adkharId":"13",
        "text": "أذكار الأذان"
      },
      {
        "adkharId":"14",
        "text": "ما يقول الإمام"
      },
      {
        "adkharId":"15",
        "text": "ما يقول بعد تكبيرة الإحرام"
      },
      {
        "adkharId":"16",
        "text": "التعوُّذ بعد دعاء الاستفتاح"
      },
      {
        "adkharId":"17",
        "text": "ما يقال في الركوع"
      },
      {
        "adkharId":"18",
        "text": "ما يقول عند الاعتدال من الركوع"
      },
      {
        "adkharId":"19",
        "text": "ما يقول في السجود"
      },
      {
        "adkharId":"20",
        "text": "ما يقول بين السجدتين"
      },
      {
        "adkharId":"21",
        "text": "ما يقول في القنوت"
      },
      {
        "adkharId":"22",
        "text": "التشهد في الصلاة"
      },
      {
        "adkharId":"23",
        "text": "الصلاة على النبي بعد التشهُّد"
      },
      {
        "adkharId":"24",
        "text": "الدعاء إذا فرغ من التشهُّد"
      },
      {
        "adkharId":"25",
        "text": "آخر ما يقال بين التشهُّد والتسليم"
      },
      {
        "adkharId":"26",
        "text": "ما يقول بعد الانصراف من الصلاة"
      },
      {
        "adkharId":"27",
        "text": "ما يقال بعد صلاة الوتر"
      },
      {
        "adkharId":"28",
        "text": "دعاء صلاة الاستخارة"
      },
      {
        "adkharId":"29",
        "text": "الدعاء عند رؤية هلال رمضان"
      },
      {
        "adkharId":"30",
        "text": "ما يقول الصائم إذا سابَّه أحد"
      },
      {
        "adkharId":"31",
        "text": "الدعاء عند إفطار الصائم"
      },
      {
        "adkharId":"32",
        "text": "الدعاء إذا أفطر عند أهل بيت"
      },
      {
        "adkharId":"33",
        "text": "الذكر قبل الطعام"
      },
      {
        "adkharId":"34",
        "text": "الدعاء عند الفراغ من الطعام"
      },
      {
        "adkharId":"35",
        "text": "دعاء الضيف لصاحب الطعام"
      },
      {
        "adkharId":"36",
        "text": "الدعاء لمن أطعَمه أو سقاه"
      },
      {
        "adkharId":"37",
        "text": "الدعاء لمن صنَع إليك معروفًا"
      },
      {
        "adkharId":"38",
        "text": "الدعاء لمن قال: إني أحبك في الله"
      },
      {
        "adkharId":"39",
        "text": "ما يقال في المجلس"
      },
      {
        "adkharId":"40",
        "text": "كفارة المجلس"
      },
      {
        "adkharId":"41",
        "text": "كيف يُلبي المُحرِم في الحج أو العمرة"
      },
      {
        "adkharId":"42",
        "text": "التكبير إذا أتى الركن الأسود"
      },
      {
        "adkharId":"43",
        "text": "الدعاء بين الركن اليماني والحجر الأسود"
      },
      {
        "adkharId":"44",
        "text": "دعاء الوقوف على الصفا والمروة"
      },
      {
        "adkharId":"45",
        "text": "الدعاء يوم عرفة"
      },
      {
        "adkharId":"46",
        "text": "الذكر عند المشعر الحرام"
      },
      {
        "adkharId":"47",
        "text": "التكبير عند رمْي الجِمار مع كل حَصاة"
      },
      {
        "adkharId":"48",
        "text": "دعاء ركوب الدابة ونحوها"
      },
      {
        "adkharId":"49",
        "text": "دعاء السفر"
      },
      {
        "adkharId":"50",
        "text": "الدعاء عند سماع صِياح الدِّيك ونَهيق الحمار"
      },
      {
        "adkharId":"51",
        "text": "الدعاء عند سماع نُباح الكلاب"
      },
      {
        "adkharId":"52",
        "text": "دعاء لقاء العدو"
      },
      {
        "adkharId":"53",
        "text": "ما يقول من خاف قومًا"
      },
      {
        "adkharId":"54",
        "text": "دعاء الريح"
      },
      {
        "adkharId":"55",
        "text": "الدعاء إذا نزَل المطر"
      },
      {
        "adkharId":"56",
        "text": "وبعد انتهاء المطر يقول"
      },
      {
        "adkharId":"57",
        "text": "الدعاء لطرْد الشيطان ووساوسه"
      },
      {
        "adkharId":"58",
        "text": "ما يُعوَّذ به الأولاد"
      },
      {
        "adkharId":"59",
        "text": "ما يقول مَن أحسَّ وجعًا في جسده"
      },
      {
        "adkharId":"60",
        "text": "الدعاء عند زيارة المريض"
      },
      {
        "adkharId":"61",
        "text": "دعاء العُطاس"
      },
      {
        "adkharId":"62",
        "text": "تلقين المُحتضَر"
      },
      {
        "adkharId":"63",
        "text": "دعاء مَن أُصيب بمصيبة"
      },
      {
        "adkharId":"64",
        "text": "دعاء التعزية"
      },
      {
        "adkharId":"65",
        "text": "دعاء قضاء الدَّين"
      },
      {
        "adkharId":"66",
        "text": "الدعاء عند الغضب"
      },
      {
        "adkharId":"67",
        "text": "دعاء الهم والحزن"
      },
      {
        "adkharId":"68",
        "text": "دعاء الكَرب"
      }
    ]
  
  